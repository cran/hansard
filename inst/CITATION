year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{hansard}: Accessing Westminster Parliament Data",
         author = c(person("Evan", "Odell")),
         year = year,
         note = note,
         url = "https://github.com/EvanOdell/hansard/")
